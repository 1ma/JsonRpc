{
    "name": "uma/json-rpc",
    "description": "a JSON-RPC 2.0 server",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "opis/json-schema": "^1.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "uma/dic": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "UMA\\JsonRpc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UMA\\JsonRpc\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "php -dzend.assertions=1 vendor/bin/phpunit --testdox"
    },
    "config": {
        "sort-packages": true
    }
}
