{
    "name": "uma/json-rpc",
    "description": "a JSON-RPC 2.0 server",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "league/json-guard": "^1.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "league/container": "3.x-dev",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "UMA\\JsonRpc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UMA\\JsonRpc\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "@composer install",
            "php -dzend.assertions=1 vendor/bin/phpunit"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
