{
    "name": "uma/rpc",
    "description": "JSON-RPC 2.0 implementation",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "league/json-guard": "^1.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "UMA\\RPC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UMA\\RPC\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
